{
  "compilerOptions": {
    "strict": true,
    "jsx": "react-native",
    "module": "esnext",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true
  },
  "baseUrl": "./",
  "paths": {
    "@component/*": [
      "./src/component/*"
    ],
    "@constants/*": [
      "./src/constants/*"
    ],
    "@pages/*": [
      "./src/pages/*"
    ],
    "@utils/*": [
      "./src/utils/*"
    ]
  },
  "extends": "expo/tsconfig.base"
}
